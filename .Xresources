URxvt.geometry: 135x40
URxvt.font: xft:Hack:size=16
URxvt.scrollBar: false
URxvt.iso14755_52: false
URxvt.iso14755: false

! Tabbing Menu Colours
URxvt.tabbed.tabbar-bg:     0
URxvt.tabbed.tabbar-fg:     6
URxvt.tabbed.tab-bg:        0
URxvt.tabbed.tab-fg:        5

! Tabbedex configs
URxvt.tabbed.autohide: true
URxvt.tabbed.reopen-on-close: yes
!URxvt.keysym.Control-t: perl:tabbedex:new_tab
!URxvt.keysym.Control-Tab: perl:tabbedex:next_tab
!URxvt.keysym.Control-Shift-Tab: perl:tabbedex:prev_tab

! Extensions
! Check if I should install clipboard
! NOTE resize-font conflicts with tabbed/tabbedex; the window doesnt redraw itself
URxvt.perl-ext-common: default,font-size
URxvt.keysym.C-Up: font-size:increase
URxvt.keysym.C-Down: font-size:decrease
!URxvt.keysym.F11: perl:fullscreen:switch
!URxvt.keysym.Shift-Control-V: perl:clipboard:paste

! Fake Transperancy
!URxvt.transparent: true
!URxvt.shading: 15

